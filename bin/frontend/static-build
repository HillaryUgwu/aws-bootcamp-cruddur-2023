REACT_APP_AWS_PROJECT_REGION="$AWS_DEFAULT_REGION" \
REACT_APP_AWS_COGNITO_REGION="$AWS_DEFAULT_REGION" \
REACT_APP_AWS_USER_POOLS_ID="us-east-1_WUU4d4exl" \
REACT_APP_CLIENT_ID="303i8fkfl7vv2uaf0gs7n5cfuu" \
npm run build